<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane fx:id="tabPaneMain" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="1000.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE"
         xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="com.poterion.monitor.ui.ConfigurationController">
    <tabs>
        <Tab fx:id="tabCommon" closable="false" text="Common">
            <content>
                <SplitPane fx:id="splitPane" dividerPositions="0.3" prefHeight="160.0" prefWidth="200.0">
                    <items>
                        <TreeView fx:id="tree" prefHeight="200.0" prefWidth="200.0"/>
                        <VBox fx:id="vboxContent">
                            <children>
                                <GridPane fx:id="gridPane" hgap="5.0" maxWidth="1.7976931348623157E308">
                                    <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                            <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" />
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label text="Type" GridPane.halignment="RIGHT" />
                              <ComboBox maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
                              <Label text="Name" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                              <Label text="Enabled" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                              <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <CheckBox mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </GridPane>
                      </children>
                  </VBox>
              </items>
            </SplitPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
